{
  "name": "wealthrun-backend",
  "version": "1.0.0",
  "description": "Backend API for WealthRun",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "prisma generate && node src/server.js",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/prisma/schema.prisma"
  },
  "keywords": ["express", "api", "wealthrun"],
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "pg": "^8.16.0",
    "prisma": "^6.14.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
